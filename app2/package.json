{
  "name": "rai-ux",
  "version": "1.0.0",
  "description": "RAI UX",
  "respository": {
    "type": "git",
    "url": "git://github.com/relationalai/rai-ux"
  },
  "author": "team@relational.ai",
  "dependencies": {
    "codemirror": "5.58.3",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "immutable": "^4.0.0-rc.12",
    "litjsx": "robbear/litjsx",
    "rai_db_sdk": "robbear/rai_db_sdk",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "request": "2.88.2",
    "request-ip": "2.1.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "eslint-webpack-plugin": "^2.4.3",
    "husky": "^5.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-axe": "4.1.0",
    "prettier": "2.2.1",
    "prettier-plugin-jsdoc": "^0.3.8",
    "style-loader": "^2.0.0",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0"
  },
  "engines": {
    "node": "14.15.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "src/node_modules/(?!@sapper)"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "scripts": {
    "build": "npm run clean && node tasks/buildTimestamp.js && npm run webpack",
    "clean": "rm -rf client/generated && mkdir -p client/generated",
    "devbuild": "npm run clean && node tasks/buildTimestamp.js && npm run webpack-dev",
    "start": "node server/app.js",
    "webpack": "webpack --config webpack.config.js",
    "test": "jest",
    "webpack-dev": "webpack --config webpack.config.dev.js",
    "format": "prettier --write .",
    "lint": "eslint --cache . --ext js",
    "postinstallIGNORE": "npx husky install",
    "heroku-postbuild": "cd app2 && npm install && rm -rf client/generated && mkdir -p client/generated && node tasks/buildTimestamp.js && webpack --config webpack.config.js && cd .."  }
}
